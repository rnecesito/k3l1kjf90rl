<RelativeLayout xmlns:android="http://schemas.android.com/apk/res/android"
    android:orientation="vertical"
    android:layout_width="match_parent"
    android:layout_height="match_parent"
    android:id="@+id/new_registration_main">

    <RelativeLayout
        android:layout_width="match_parent"
        android:layout_height="match_parent"
        android:layout_marginLeft="5dp"
        android:layout_marginTop="80dp"
        android:layout_marginRight="80dp"
        android:layout_marginBottom="90dp"
        android:background="@drawable/container_background"
        android:id="@+id/relativeLayout">

        <Button
            android:layout_width="90dp"
            android:layout_height="60dp"
            android:text="@string/save"
            android:id="@+id/saveB"
            android:enabled="true"
            android:textAllCaps="false"
            android:background="@drawable/square_title"
            android:textColor="#ffffffff"
            android:textSize="25sp"
            android:layout_marginTop="17dp"
            android:layout_alignParentTop="true"
            android:layout_alignParentRight="true"
            android:clickable="true" />

        <TextView
            android:layout_width="110dp"
            android:id="@+id/registerTv"
            android:layout_alignParentTop="true"
            android:text="@string/text_party_registration"
            android:layout_alignParentLeft="true"
            android:layout_marginTop="17dp"
            android:textSize="15sp"
            android:textColor="@android:color/white"
            android:gravity="center"
            android:layout_height="60dp" />

        <ScrollView
            android:layout_width="match_parent"
            android:layout_below="@id/registerTv"
            android:layout_height="match_parent">

            <RelativeLayout
                android:layout_width="match_parent"
                android:layout_below="@+id/registerTv"
                android:layout_height="wrap_content">

                <TextView
                    android:layout_width="wrap_content"
                    android:layout_height="wrap_content"
                    android:textAppearance="?android:attr/textAppearanceMedium"
                    android:text="@string/jap_party_name"
                    android:id="@+id/textView123"
                    android:layout_marginLeft="29dp"
                    android:layout_marginStart="29dp"
                    android:textSize="25sp"
                    android:autoText="false"
                    android:textColor="#ffffffff"
                    android:layout_alignParentLeft="false"
                    android:layout_alignParentStart="true"
                    android:layout_marginTop="9dp" />

                <EditText
                    android:layout_width="240dp"
                    android:layout_height="35dp"
                    android:id="@+id/party_name_value"
                    android:background="@drawable/container_background_3"
                    android:layout_marginTop="8dp"
                    android:layout_below="@+id/textView123"
                    android:layout_alignLeft="@+id/textView123"
                    android:layout_alignParentEnd="false"
                    android:layout_alignParentStart="false"
                    android:inputType="textEmailAddress"
                    android:textColor="#ff000000"
                    android:paddingLeft="5dp" />

                <TextView
                    android:layout_width="wrap_content"
                    android:layout_height="wrap_content"
                    android:textAppearance="?android:attr/textAppearanceMedium"
                    android:text="@string/jap_party_date"
                    android:id="@+id/partyDateText"
                    android:layout_marginLeft="29dp"
                    android:layout_marginStart="29dp"
                    android:textSize="25sp"
                    android:autoText="false"
                    android:textColor="#ffffffff"
                    android:layout_below="@+id/party_name_value"
                    android:layout_alignLeft="@+id/party_name_value"
                    android:layout_alignParentLeft="false"
                    android:layout_alignParentStart="true"
                    android:layout_marginTop="9dp" />

                <EditText
                    android:layout_width="240dp"
                    android:layout_height="35dp"
                    android:id="@+id/party_date_value"
                    android:background="@drawable/container_background_3"
                    android:layout_marginTop="8dp"
                    android:layout_below="@+id/partyDateText"
                    android:layout_alignLeft="@+id/partyDateText"
                    android:layout_alignParentEnd="false"
                    android:layout_alignParentStart="false"
                    android:inputType="textEmailAddress"
                    android:text="MM/dd/yy"
                    android:textColor="#ff000000"
                    android:editable="false"
                    android:paddingLeft="5dp" />

                <TextView
                    android:layout_width="wrap_content"
                    android:layout_height="wrap_content"
                    android:textAppearance="?android:attr/textAppearanceMedium"
                    android:text="@string/text_course_name"
                    android:id="@+id/textView"
                    android:layout_marginLeft="29dp"
                    android:layout_marginStart="29dp"
                    android:textSize="25sp"
                    android:autoText="false"
                    android:textColor="#ffffffff"
                    android:layout_below="@+id/party_date_value"
                    android:layout_alignLeft="@+id/party_date_value"
                    android:layout_alignParentLeft="false"
                    android:layout_alignParentStart="true"
                    android:layout_marginTop="9dp" />

                <Spinner
                    android:layout_width="240dp"
                    android:layout_height="35dp"
                    android:id="@+id/courseSpinner"
                    android:layout_marginTop="8dp"
                    android:layout_below="@+id/textView"
                    android:layout_alignLeft="@+id/textView"
                    android:layout_alignRight="@+id/editTextPassword"
                    android:layout_alignParentEnd="false"
                    android:layout_alignParentStart="false"
                    android:paddingLeft="5dp"
                    android:spinnerMode="dropdown" />

                <TextView
                    android:layout_width="wrap_content"
                    android:layout_height="wrap_content"
                    android:textAppearance="?android:attr/textAppearanceMedium"
                    android:text="@string/fellow_competitor_name"
                    android:id="@+id/textView2"
                    android:textSize="25dp"
                    android:autoText="false"
                    android:textColor="#ffffffff"
                    android:layout_below="@+id/courseSpinner"
                    android:layout_alignLeft="@+id/cName1"
                    android:layout_alignStart="@+id/cName1" />

                <!--competitor 1-->
                <Spinner
                    android:layout_width="240dp"
                    android:layout_height="35dp"
                    android:id="@+id/cName1"
                    android:layout_below="@+id/textView2"
                    android:layout_alignLeft="@+id/courseSpinner"
                    android:layout_marginTop="8dp"
                    android:textColor="#ff000000"
                    android:paddingLeft="5dp"
                    android:spinnerMode="dropdown" />

                <TextView
                    android:layout_width="wrap_content"
                    android:layout_height="wrap_content"
                    android:text="@string/not_entered"
                    android:layout_marginTop="5dp"
                    android:layout_marginLeft="5dp"
                    android:textColor="@android:color/holo_red_dark"
                    android:layout_below="@+id/cName1"
                    android:visibility="invisible"
                    android:id="@+id/textViewV1" />

                <TextView
                    android:layout_width="wrap_content"
                    android:layout_height="wrap_content"
                    android:text="Handicap"
                    android:textAppearance="?android:attr/textAppearanceMedium"
                    android:layout_marginLeft="30dp"
                    android:layout_below="@+id/textViewV1"
                    android:id="@+id/textView25" />

                <ImageView
                    android:layout_width="wrap_content"
                    android:layout_height="wrap_content"
                    android:id="@+id/imgMinusB"
                    android:src="@drawable/ic_triangle_minus"
                    android:layout_below="@+id/textViewV1"
                    android:layout_marginLeft="10dp"
                    android:layout_toRightOf="@+id/textView25"
                />

                <TextView
                    android:layout_width="wrap_content"
                    android:layout_height="wrap_content"
                    android:text="0"
                    android:textAppearance="?android:attr/textAppearanceMedium"
                    android:layout_toRightOf="@+id/imgMinusB"
                    android:layout_below="@+id/textViewV1"
                    android:layout_marginLeft="10dp"
                    android:layout_marginRight="20dp"
                    android:padding="10dp"
                    android:background="@drawable/container_background_4"
                    android:id="@+id/textViewC1Count" />

                <ImageView
                    android:layout_width="wrap_content"
                    android:layout_height="wrap_content"
                    android:id="@+id/imgAddB"
                    android:src="@drawable/ic_triangle_add"
                    android:layout_below="@+id/textViewV1"
                    android:layout_marginLeft="10dp"
                    android:layout_toRightOf="@+id/textViewC1Count"
                    />

                <!--competitor 2-->
                <Spinner
                    android:layout_width="240dp"
                    android:layout_height="35dp"
                    android:id="@+id/cName2"
                    android:textColor="#ff000000"
                    android:paddingLeft="5dp"
                    android:layout_below="@+id/imgMinusB"
                    android:layout_marginTop="10dp"
                    android:layout_alignLeft="@+id/textView25"
                    android:layout_alignStart="@+id/textView25"
                    android:spinnerMode="dropdown" />

                <TextView
                    android:layout_width="wrap_content"
                    android:layout_height="wrap_content"
                    android:text="@string/not_entered"
                    android:textColor="@android:color/holo_red_dark"
                    android:id="@+id/textViewV2"
                    android:layout_below="@+id/cName2"
                    android:visibility="invisible"
                    android:layout_alignLeft="@+id/textViewV1"
                    android:layout_alignStart="@+id/textViewV1" />

                <TextView
                    android:layout_width="wrap_content"
                    android:layout_height="wrap_content"
                    android:text="Handicap"
                    android:textAppearance="?android:attr/textAppearanceMedium"
                    android:layout_marginLeft="30dp"
                    android:layout_below="@+id/textViewV2"
                    android:id="@+id/textView27" />

                <ImageView
                    android:layout_width="wrap_content"
                    android:layout_height="wrap_content"
                    android:id="@+id/imgMinusB2"
                    android:src="@drawable/ic_triangle_minus"
                    android:layout_below="@+id/textViewV2"
                    android:layout_marginLeft="10dp"
                    android:layout_toRightOf="@+id/textView27"
                    />

                <TextView
                    android:layout_width="wrap_content"
                    android:layout_height="wrap_content"
                    android:text="0"
                    android:textAppearance="?android:attr/textAppearanceMedium"
                    android:layout_toRightOf="@+id/imgMinusB2"
                    android:layout_below="@+id/textViewV2"
                    android:layout_marginLeft="10dp"
                    android:layout_marginRight="20dp"
                    android:padding="10dp"
                    android:background="@drawable/container_background_4"
                    android:id="@+id/textViewC2Count" />

                <ImageView
                    android:layout_width="wrap_content"
                    android:layout_height="wrap_content"
                    android:id="@+id/imgAddB2"
                    android:src="@drawable/ic_triangle_add"
                    android:layout_below="@+id/textViewV2"
                    android:layout_marginLeft="10dp"
                    android:layout_toRightOf="@+id/textViewC2Count"
                    />

                <!--compeitor 3-->
                <Spinner
                    android:layout_width="240dp"
                    android:layout_height="35dp"
                    android:id="@+id/cName3"
                    android:textColor="#ff000000"
                    android:paddingLeft="5dp"
                    android:layout_below="@+id/imgMinusB2"
                    android:layout_marginTop="10dp"
                    android:layout_alignLeft="@+id/textView25"
                    android:layout_alignStart="@+id/textView25"
                    android:spinnerMode="dropdown" />

                <TextView
                    android:layout_width="wrap_content"
                    android:layout_height="wrap_content"
                    android:text="@string/not_entered"
                    android:textColor="@android:color/holo_red_dark"
                    android:id="@+id/textViewV3"
                    android:layout_below="@+id/cName3"
                    android:visibility="invisible"
                    android:layout_alignLeft="@+id/textViewV1"
                    android:layout_alignStart="@+id/textViewV1" />

                <TextView
                    android:layout_width="wrap_content"
                    android:layout_height="wrap_content"
                    android:text="Handicap"
                    android:textAppearance="?android:attr/textAppearanceMedium"
                    android:layout_marginLeft="30dp"
                    android:layout_below="@+id/textViewV3"
                    android:id="@+id/textView30" />

                <ImageView
                    android:layout_width="wrap_content"
                    android:layout_height="wrap_content"
                    android:id="@+id/imgMinusB3"
                    android:src="@drawable/ic_triangle_minus"
                    android:layout_below="@+id/textViewV3"
                    android:layout_marginLeft="10dp"
                    android:layout_toRightOf="@+id/textView30"
                    />

                <TextView
                    android:layout_width="wrap_content"
                    android:layout_height="wrap_content"
                    android:text="0"
                    android:textAppearance="?android:attr/textAppearanceMedium"
                    android:layout_toRightOf="@+id/imgMinusB3"
                    android:layout_below="@+id/textViewV3"
                    android:layout_marginLeft="10dp"
                    android:layout_marginRight="20dp"
                    android:padding="10dp"
                    android:background="@drawable/container_background_4"
                    android:id="@+id/textViewC3Count" />

                <ImageView
                    android:layout_width="wrap_content"
                    android:layout_height="wrap_content"
                    android:id="@+id/imgAddB3"
                    android:src="@drawable/ic_triangle_add"
                    android:layout_below="@+id/textViewV3"
                    android:layout_marginLeft="10dp"
                    android:layout_toRightOf="@+id/textViewC3Count"
                    android:layout_marginBottom="20dp"
                    />

                <!--competitor 4-->
                <Spinner
                    android:layout_width="240dp"
                    android:layout_height="35dp"
                    android:id="@+id/cName4"
                    android:textColor="#ff000000"
                    android:paddingLeft="5dp"
                    android:layout_below="@+id/imgMinusB3"
                    android:layout_marginTop="10dp"
                    android:layout_alignLeft="@+id/textView27"
                    android:layout_alignStart="@+id/textView27"
                    android:spinnerMode="dropdown" />

                <TextView
                    android:layout_width="wrap_content"
                    android:layout_height="wrap_content"
                    android:text="@string/not_entered"
                    android:textColor="@android:color/holo_red_dark"
                    android:id="@+id/textViewV4"
                    android:layout_below="@+id/cName4"
                    android:visibility="invisible"
                    android:layout_alignLeft="@+id/textViewV4"
                    android:layout_alignStart="@+id/textViewV4" />

                <TextView
                    android:layout_width="wrap_content"
                    android:layout_height="wrap_content"
                    android:text="Handicap"
                    android:textAppearance="?android:attr/textAppearanceMedium"
                    android:layout_marginLeft="30dp"
                    android:layout_below="@+id/textViewV4"
                    android:id="@+id/textView31" />
                <ImageView
                    android:layout_width="wrap_content"
                    android:layout_height="wrap_content"
                    android:id="@+id/imgMinusB4"
                    android:src="@drawable/ic_triangle_minus"
                    android:layout_below="@+id/textViewV4"
                    android:layout_marginLeft="10dp"
                    android:layout_toRightOf="@+id/textView31"
                    />
                <TextView
                    android:layout_width="wrap_content"
                    android:layout_height="wrap_content"
                    android:text="0"
                    android:textAppearance="?android:attr/textAppearanceMedium"
                    android:layout_toRightOf="@+id/imgMinusB4"
                    android:layout_below="@+id/textViewV4"
                    android:layout_marginLeft="10dp"
                    android:layout_marginRight="20dp"
                    android:padding="10dp"
                    android:background="@drawable/container_background_4"
                    android:id="@+id/textViewC4Count" />

                <ImageView
                    android:layout_width="wrap_content"
                    android:layout_height="wrap_content"
                    android:id="@+id/imgAddB4"
                    android:src="@drawable/ic_triangle_add"
                    android:layout_below="@+id/textViewV4"
                    android:layout_marginLeft="10dp"
                    android:layout_toRightOf="@+id/textViewC4Count"
                    android:layout_marginBottom="20dp"
                    />

            </RelativeLayout>

        </ScrollView>

    </RelativeLayout>

    <Button
        android:layout_width="120dp"
        android:layout_height="40dp"
        android:text="@string/title_menu"
        android:textAllCaps="false"
        android:id="@+id/menu_button"
        android:layout_alignParentTop="true"
        android:layout_alignParentLeft="true"
        android:layout_alignParentStart="true"
        android:background="#ff313131"
        android:layout_marginTop="20dp"
        android:elegantTextHeight="false"
        android:enabled="true"
        android:textStyle="normal"
        android:singleLine="true"
        android:textSize="27sp" />
    <Button
        android:layout_width="120dp"
        android:layout_height="30dp"
        android:text="@string/logout"
        android:textAllCaps="false"
        android:id="@+id/logout_button"
        android:background="#ff313131"
        android:elegantTextHeight="false"
        android:enabled="true"
        android:textStyle="normal"
        android:singleLine="true"
        android:textSize="22sp"
        android:layout_marginBottom="49dp"
        android:layout_alignParentBottom="true"
        android:layout_alignLeft="@+id/dd"
        android:layout_alignStart="@+id/dd" />

</RelativeLayout>
